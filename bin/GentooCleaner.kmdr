# visual part
@TempGroup.setEnabled(false)
@DoneButton.setvisible(false)
@AdvancedCheckBox.setVisible(false)

# Not get the info
@ConfProgressBar.setMaximum(8)
@ConfProgressBar.setVisible(true)

@ConfProgressBar.setText("1")
@exec(emerge --info &gt; /tmp/gentoo_kmdr.info)
@ConfProgressBar.setText("2")
@setGlobal(DISTDIR, @exec(grep DISTDIR &lt; /tmp/gentoo_kmdr.info | sed 's/DISTDIR=//' | sed 's/"//g' ))
@DistfilesLabel.setText(@global(DISTDIR))
@setGlobal(TEMPDIR, @exec(grep PORTAGE_TMPDIR &lt; /tmp/gentoo_kmdr.info | sed 's/PORTAGE_TMPDIR=//' | sed 's/"//g' ))
@GentooTempLabel.setText(@global(TEMPDIR))
@setGlobal(PACKAGESDIR, @exec(grep PKGDIR &lt; /tmp/gentoo_kmdr.info | sed 's/PKGDIR=//' | sed 's/"//g' ))
@PackagesLabel.setText(@global(PACKAGESDIR))
@setGlobal(GENKERNELDIR, "/var/tmp/genkernel")
@GenkernelTempLabel.setText(@global(GENKERNELDIR))
@exec(rm -f /tmp/gentoo_kmdr.info)

@ConfProgressBar.setText("3")
@Distfiles.setText( @exec(du -ksh @global(DISTDIR) | awk '{print $1}') )
@ConfProgressBar.setText("4")
@GenkernelTemp.setText( @exec(du -ksh @global(GENKERNELDIR) | awk '{print $1}') )
@ConfProgressBar.setText("5")
@GentooTemp.setText( @exec(du -ksh @global(TEMPDIR) | awk '{print $1}') )
@ConfProgressBar.setText("6")
@Packages.setText( @exec(du -ksh @global(PACKAGESDIR) | awk '{print $1}') )

@ConfProgressBar.setText("7")
@setGlobal(NBLOCKS, @exec(ls -aR @global(TEMPDIR) | grep portage_lockfile | wc -l))
@if( @exec( test @global(NBLOCKS) != 0 &amp;&amp; echo 1 ) )
	@GentooTempCleanup.setEnabled(false)
	@GentooTemp.setText("@GentooTemp.text() - locked")
@endif
@GentooTemp.setText("@GentooTemp.text() - @global(NBLOCKS)")


@ConfProgressBar.setVisible(false)
@DoneButton.setVisible(true)
@TempGroup.setEnabled(true)
@AdvancedCheckBox.setVisible(true)

