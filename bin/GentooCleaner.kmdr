                    <property name="associations" stdset="0">
                        <stringlist>
                            <string>
dcop @dcopid KommanderIf setVisible AdvancedCheckBox false
dcop @dcopid KommanderIf setVisible ConfProgressBar true
dcop @dcopid KommanderIf setText ConfProgressBar "0"

MaxItems=`ls @global(CLEANUPDIRS) | grep ':' | wc -l`
dcop @dcopid KommanderIf setMaximum ConfProgressBar $MaxItems

CurrentItem=1
for dir in `ls @global(CLEANUPDIRS) | grep ':' | sed 's/://g'` ; do
	rm -rf $dir
	dcop @dcopid KommanderIf setText ConfProgressBar "$CurrentItem"
	(( CurrentItem = CurrentItem + 1 ))
done

dcop @dcopid KommanderIf setText ConfProgressBar "0"
dcop @dcopid KommanderIf setVisible ConfProgressBar false
dcop @dcopid KommanderIf setVisible AdvancedCheckBox true
</string>
